<h1> Login Here </h1>

<%= render partial: "layouts/flash_errors", locals: {object: @subscription} %>

<%= form_with url: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit 'Login' %>
    
<% end %>

<h1>Or Signup</h1>
<%= link_to 'Sign Up', signup_path %> 

<h1>Or Login with Google:</h1>

<%= button_to 'Google', '/auth/google_oauth2' %>